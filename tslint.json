{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "no-console": false,
    "object-literal-sort-keys": [
      false,
      "match-declaration-order",
      "shorthand-first"
    ],
    "quotemark": [true, "single"]
  },
  "rulesDirectory": []
}